@page "/Gallery"
@inject IJSRuntime Js

<MudPaper Style="width: 100%; height:100vh; padding: 0; margin: 0">
    <br/>
    <iframe style="width:100%; height:94vh;" src="@(@"https://i.simmer.io/@postcarbon/postcarbon-virtual-gallery")"></iframe>
</MudPaper>

@code {
    [CascadingParameter]
    MainLayout MainLayout { get; set; }
    
    protected override async Task OnInitializedAsync()
    {
        MainLayout.SetIsIndex(false);
    }
}